>
    <skos:ConceptScheme rdf:about="https://metadata.pithia.eu/ontology/2.2/serviceFunction">
        <skos:prefLabel xml:lang="en">PITHIA Service Function</skos:prefLabel>
        <dc:description>This vocabulary is used in the PITHIA_Result object (throught PITHIA_Observation) to describe the Service Function.</dc:description>
        <dc:date>2013-05-31 10:00:00.0</dc:date>
        <owlxml:versionInfo>0.1</owlxml:versionInfo>
        <dc:publisher>PITHIA</dc:publisher>
